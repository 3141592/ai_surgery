{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "TF: Debug current file",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "cwd": "${fileDirname}",
      "console": "integratedTerminal",
      "python": "${userHome}/venvs/tf/bin/python",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/.."
      }
    },
    {
      "name": "Torch: Debug current file",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "cwd": "${fileDirname}",
      "python": "${userHome}/venvs/torch/bin/python",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/.."
      }
    }
  ]
}

